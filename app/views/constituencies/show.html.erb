<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @constituency.name %>
</p>

<p>
  <strong>Voters:</strong>
  <%= @constituency.voters %>
</p>

<p>
  <strong>Voivodeship:</strong>
  <%= link_to @constituency.voivodeship.name, @constituency.voivodeship %>
</p>

<p>
  <strong>Votes:</strong>
  <% @constituency.votes.each do |v| %>
  <li><%= link_to v.comittee.name, v.comittee %> <%= v.quantity %>
  	<% end %>
</p>

<%= link_to 'Edit', edit_constituency_path(@constituency) %> |
<%= link_to 'Back', constituencies_path %>
